{{#with moves}}
{{#if starting.[0]}}
<section class="mon_moves">
  <h3 class="moves_heading">Moves</h3>

  <table class="moves_table">
    <thead>
      <th>Level
      <th>Learns
    </thead>
    {{#if form}}
    <tr>
      <th>Form
      <td>{{#each form}}
            {{#each oneOf~}}
              <a href="/ptv/moves/#{{slugify this}}">{{this}}</a>
              {{~#unless @last}}&thinsp;/&thinsp;{{/unless~}}
            {{~else}}
              <a href="/ptv/moves/#{{slugify this}}">{{this}}</a>
              {{~#unless @last}}, {{/unless}}
            {{/each}}
          {{/each}}
    </tr>
    {{/if}}
    <tr>
      <th>1
      <td>{{#each starting}}
            {{#each oneOf~}}
              <a href="/ptv/moves/#{{slugify this}}">{{this}}</a>
              {{~#unless @last}}&thinsp;/&thinsp;{{/unless~}}
            {{~else}}
              <a href="/ptv/moves/#{{slugify this}}">{{this}}</a>
              {{~#unless @last}}, {{/unless}}
            {{/each}}
          {{/each}}
    </tr>
    {{#each levelUp}}
    <tr>
      <th>{{@key}}
      <td>{{#each this}}
            {{#each oneOf~}}
              <a href="/ptv/moves/#{{slugify this}}">{{this}}</a>
              {{~#unless @last}}&thinsp;/&thinsp;{{/unless~}}
            {{~else}}
              <a href="/ptv/moves/#{{slugify this}}">{{this}}</a>
              {{~#unless @last}}, {{/unless}}
            {{/each}}
          {{else}}
            â€”
          {{/each}}
    {{/each}}
  </table>

  <section class="tutor-moves">
    <h4 class="tutor-moves_heading">Tutor Moves</h4>

    <ul class="tutor-moves_list">
    {{#each tutor}}
      <li><a href="/ptv/moves/#{{slugify this}}">{{this}}</a>
    {{/each}}
    </ul>
  </section>
</section>
{{/if}}
{{/with}}